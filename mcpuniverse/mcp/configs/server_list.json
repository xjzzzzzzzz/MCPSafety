{
  "date": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.date"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.date",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    }
  },
  "weather": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.weather"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.weather",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    }
  },
  "echo": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.echo"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.echo",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    }
  },
  "terminal": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.terminal"
      ]
    }
  },
  "google-search": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.google_search"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.google_search",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    },
    "env": {
      "SERP_API_KEY": "{{SERP_API_KEY}}"
    }
  },
  "wikipedia": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.wikipedia"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.wikipedia",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    }
  },
  "google-maps": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.google_maps"
      ]
    },
    "env": {
      "GOOGLE_MAPS_API_KEY": "{{GOOGLE_MAPS_API_KEY}}"
    }
  },
  "fetch": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_fetch",
        "--ignore-robots-txt"
      ]
    }
  },
  "no_hub": {
    "stdio": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ]
    },
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "{{GITHUB_PERSONAL_ACCESS_TOKEN}}"
    }
  },
  "github": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.github"
      ]
    }
  },
  "google-sheets": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.google_sheets"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.google_sheets",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    },
    "env": {
      "SERVICE_ACCOUNT_PATH": "{{GOOGLE_SERVICE_ACCOUNT_PATH}}",
      "GOOGLE_DRIVE_FOLDER_ID": "{{GOOGLE_DRIVE_FOLDER_ID}}"
    }
  },
  "filesystem": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "{{FILESYSTEM_DIRECTORY}}"
      ]
    }
  },
  "slack": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-slack"
      ]
    },
    "env": {
      "SLACK_BOT_TOKEN": "{{SLACK_BOT_TOKEN}}",
      "SLACK_TEAM_ID": "{{SLACK_TEAM_ID}}"
    }
  },
  "postgres": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "{{POSTGRES_ADDRESS}}"
      ]
    }
  },
  "calculator": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_calculator"
      ]
    }
  },
  "yfinance": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.yahoo_finance"
      ]
    },
    "sse": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.yahoo_finance",
        "--transport",
        "sse",
        "--port",
        "{{PORT}}"
      ]
    }
  },
  "notion": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@notionhq/notion-mcp-server"
      ]
    },
    "env": {
      "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer {{NOTION_API_KEY}}\", \"Notion-Version\": \"2022-06-28\" }"
    }
  },
  "playwright": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.playwright"
      ]
    }
  },
  "markitdown": {
    "stdio": {
      "command": "markitdown-mcp"
    }
  },
  "paypal": {
    "stdio": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://mcp.paypal.com/sse"
      ]
    }
  },
  "chroma": {
    "stdio": {
      "command": "python",
      "args": [
        "-m",
        "mcpuniverse.mcp.servers.chroma"
      ]
    }
  }
}